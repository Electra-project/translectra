{
  "name": "translectra",
  "version": "0.3.0",
  "description": "Open-source Translation Management System.",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=10.2.1",
    "npm": ">=6.1.0"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "prestart": "npm run build",
    "start": "node build/index.js",
    "start:dev": "npm run watch",
    "test": "cross-env mocha --require babel-register 'src/**/*.spec.js'",
    "postversion": "npm publish",
    "watch": "concurrently \"npm run watch:build\" \"npm run watch:start\"",
    "watch:build": "webpack --config webpack.dev.js --watch",
    "watch:start": "nodemon --config nodemon.json build/index.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "archiver": "^2.1.1",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "connect-flash": "^0.1.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "highlight.js": "^9.12.0",
    "jquery": "^3.3.1",
    "lexpress": "^0.39.6",
    "lodash": "^4.17.10",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.1",
    "mongoose": "^5.1.3",
    "mongoose-unique-validator": "^2.0.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local-mongoose": "^5.0.0",
    "popper.js": "^1.14.3",
    "ramda": "^0.25.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "semver-regex": "^2.0.0",
    "showdown": "^1.8.6",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2",
    "snyk": "^1.161.1"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.6",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Electra-project/translectra.git"
  },
  "keywords": [],
  "author": {
    "name": "Electra Project",
    "email": "dev@electraproject.org",
    "url": "https://electraproject.org"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/Electra-project/translectra/issues"
  },
  "homepage": "https://github.com/Electra-project/translectra#readme",
  "snyk": true
}
